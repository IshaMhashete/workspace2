package com.user;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

@Entity
@Table(name= "USER")
public class User {
	@Id
	@Column(name= "USER_ID")
	int userId;
	
	@Column(name= "USER_NAME")
	String userName;
	
	@Column(name= "EMAIL_ID")
	String emailId;
	
	@Column(name= "CONTACT_NO")
	Double contactNo;
	

}
